<p id="notice"><%= notice %></p>

<h1>Pictures</h1>

<%= link_to new_picture_path do %>
    <button class="btn btn-primary">New</button>
<% end %>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Image</th>
      <th>Action</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= render partial: "picture", collection: @pictures %>
  </tbody>
</table>

